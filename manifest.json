{
    "required_api_version": "^2.0.0",
    "name": "Session Manager",
    "description": "An extension that offers you session options",
    "developer_name": "lighttigerXIV",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 0.05
    },
    "preferences": [
        {
            "id": "kw",
            "type": "keyword",
            "name": "Session Manager",
            "default_value": "sm"
